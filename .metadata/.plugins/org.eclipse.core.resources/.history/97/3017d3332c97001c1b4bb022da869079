
public class Movie: Entity {
	public int id;
	public int year;
	public String director;
	public String genre;
	public String title;
	public int length;
	
	public Movie(int ID, int YEAR, String DIRECTOR, String GENRE, String TITLE, int LENGTH) {
		id = ID;
		year = YEAR;
		director = DIRECTOR;
		genre = GENRE;
		title = TITLE;
		length = LENGTH;
	}
}
