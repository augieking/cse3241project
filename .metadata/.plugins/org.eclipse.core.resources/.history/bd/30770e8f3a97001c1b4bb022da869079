import java.util.ArrayList;

public class Music implements Entity{
	public int id;
	public int year;
	public int length;
	public String albumName;
	public String songName;
	public ArrayList<String> artists;
	
	public Music(int ID, int YEAR, int LENGTH, String ALBUMNAME, String SONGNAME, ArrayList ARTISTS) {
		id = ID;
		year = YEAR;
		length = LENGTH;
		albumName = ALBUMNAME;
		songName = SONGNAME;
		artists = ARTISTS;
	}
	
	public void update() {
		
	}
}
